{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["@types/jest"],
    "baseUrl": ".",
    "sourceMap": true,
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@parts/*": ["./components/parts/*"]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/node",
      "@nuxtjs/auth-next"
    ]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}

